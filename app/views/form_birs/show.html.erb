<%- model_class = FormBir -%>
<div class="page-header">
  <h1><%=t '.title', :default => FormBir %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:ilk_basvuru_tarihi) %></dt>
      <dd><pre class="prettyprint"><%= @form_bir.ilk_basvuru_tarihi %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:notlar) %></dt>
      <dd><pre class="prettyprint"><%= @form_bir.notlar %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:mc_siginaginda_kaliyor) %></dt>
      <dd><pre class="prettyprint"><%= @form_bir.mc_siginaginda_kaliyor %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              form_birs_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_form_bir_path(@form_bir), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              form_bir_path(@form_bir),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

